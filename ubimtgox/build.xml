	<property name="jetty.home" value="./jetty-distribution-8.1.10.v20130312" />

    <path id="compile.ubiquisense">
        <fileset dir="libs">
            <include name="*.jar" />
            <include name="google-gson-2.2.2/*.jar" />
      	</fileset>
        <fileset dir="./plugins">
            <include name="*.jar" />
	    </fileset>
        <fileset dir="${jetty.home}">
            <include name="lib/*.jar" />
            <include name="lib/jta/*.jar" />
            <include name="lib/annotations/*.jar" />
            <include name="lib/servlet-api-*.jar" />
            <include name="lib/jsp/*.jar" />
        </fileset>
        <fileset dir="./build/classes">
            <include name="**/*.class" />
	    </fileset>
    </path>
    <path id="compile.jspc">
        <fileset dir="${jetty.home}">
            <include name="lib/servlet-api-*.jar" />
            <include name="lib/jsp/*.jar" />
        </fileset>
        <fileset dir="./build/classes">
            <include name="**/*.class" />
	    </fileset>
    </path>

	    <path id="compile.classpath">
	        <fileset dir="./webapps/WEB-INF/lib">
	            <include name="*.jar" />
	        </fileset>
	        <path refid="compile.jspc" />
	        <path refid="compile.ubiquisense" />
	    </path>

	    <target name="jspc" depends="compile">
	        <taskdef classname="org.apache.jasper.JspC" name="jasper2" classpathref="compile.jspc" />
	        <jasper2 validateXml="false"
	            uriroot="./webapps"
	            addWebXmlMappings="true"
	            webXmlFragment="./webapps/WEB-INF/generated_web.xml"
	            compilerSourceVM="1.6"
	            compilerTargetVM="1.6"
	            outputDir="src-jsp"
	            verbose="9" />
	    </target>

	    <target name="init">
	        <mkdir dir="build/classes" />
	        <mkdir dir="dist" />
	        <copy todir="src-jsp/">
	            <fileset dir="src" includes="**/*.java" />
	        </copy>
	    </target>

	    <target name="compile" depends="init">
	        <javac destdir="build/classes" debug="true" srcdir="src-mongo">
	            <classpath refid="compile.classpath" />
	        </javac>
	        <javac destdir="build/classes" debug="true" srcdir="src-main">
	            <classpath refid="compile.classpath" />
	        </javac>
	        <javac destdir="build/classes" debug="true" srcdir="src-jsp">
	            <classpath refid="compile.classpath" />
	        </javac>
	    </target>

